    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-scripts": "1.1.4",
    "react-toolbox": "^2.0.0-beta.12",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
